# Backend Environment Configuration

# API Configuration
API_HOST=localhost
API_PORT=8000
API_DEBUG=true

# AWS Configuration
AWS_PROFILE_PATH=~/.aws/
AWS_REGION=us-east-1

# Cache Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL=300

# Security
CORS_ORIGINS=["http://localhost:3000"]
API_KEY_NAME=X-API-Key

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=JSON

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60
